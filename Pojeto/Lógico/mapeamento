# Entidades


Pesquisador(CPF,instituição);

Região(Cód_região, coordenadas_latitude, coordenadas _longitude);

Bioma(Cód_região,bioma);

Cód_região -> Região(Cód_região)

Espécie(Nome_científico,tax_Reino, tax_Filo, tax_Classe, tax_Ordem, tax_Família, tax_Gênero);

Nome_popular(Nome_científico,nome_popular);

Nome_científico -> Espécie(Nome_científico)

Pesquisa(Título,resultado);

Congresso(Cód_congresso,ano);

Empresa(CNPJ,nome);

Pública(CNPJ,órgão_regulador);

CNPJ -> Empresa(CNPJ)

Privada(CNPJ,natureza_jurídica);

CNPJ-> Empresa(CNPJ)

Reserva_ambiental(Cód_região,CNPJ,Responsável);

Cód_região -> Região(Cód_região)

CNPJ -> Empresa(CNPJ)


# Relacionamentos


Estuda(CPF,Cód_região,Nome_científico,data);

CPF -> Pesquisador(CPF)

Cód_região -> Região(Cód_região)

Nome_científico -> Espécie(Nome_científico)

Descobre(Nome_científico_novo, [CPF,Cód_região, Nome_científico, data_estuda],data_descobre);

Nome_Científico_novo -> Espécie(Nome_científico)

(CPF,Cód_região,Nome_científico,data_estuda) -> Estuda(CPF,Cód_região,Nome_científico,data)

Faz(CPF,Título);

CPF -> Pesquisador(CPF)

Título -> Pesquisa(Título)

Financia(CPF,Título,CNPJ);

(CPF,Título) -> Faz(CPF,Título)

CNPJ -> Empresa(CNPJ)

Apresentada(Título,Cód_congresso);

Título -> Pesquisa(Título)

Cód_congresso -> Congresso(Cód_congresso)

Preservada(Cód_região,CNPJ,responsável,Nome_científico);

(Cód_região,CNPJ,Responsável) -> Reserva_ambiental(Cód_região,CNPJ,Responsável)

Nome_científico -> Espécie(Nome_científico)

Orienta(Aluno,Professor!)

Aluno-> Pesquisador(CPF)

Professor -> Pesquisador(CPF)
